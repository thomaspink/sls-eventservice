{
  "name": "sls-eventservice",
  "version": "0.0.1",
  "description": "Website for SLS Eventservice",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run clean",
    "build": "npm run clean && npm run lint && npm run build:server",
    "build:server": "node_modules/typescript/bin/tsc -p ./server/tsconfig.json && cp -R server/templates dist/server/templates",
    "lint": "npm run lint:ts",
    "lint:ts": "node_modules/tslint/bin/tslint -c ./tslint.json 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomaspink/sls-eventservice.git"
  },
  "author": "Thomas Pink",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thomaspink/sls-eventservice/issues"
  },
  "homepage": "https://github.com/thomaspink/sls-eventservice#readme",
  "devDependencies": {
    "rimraf": "^2.5.4",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6"
  },
  "dependencies": {
    "@types/express": "^4.0.35",
    "@types/nunjucks": "0.0.32",
    "express": "^4.14.1",
    "nunjucks": "^3.0.0"
  }
}
